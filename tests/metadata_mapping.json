{
  "default": {
    "namespaces": {
      "dc": "http://purl.org/dc/terms/",
      "ex": "http://example.com",
      "foaf": "http://xmlns.com/foaf/0.1/"
    },
    "properties": {
      "dc:title": ".//tei:titleStmt/tei:title[@type='main']",
      "dc:creator": ".//tei:titleStmt/tei:author",
      "dc:description": ".//tei:sourceDesc//tei:title[@type='subtitle']",
      "dc:date": [
        ".//tei:sourceDesc//tei:date",
        ".//tei:editionStmt//tei:date"
      ],
      "dc:publisher": ".//tei:sourceDesc//tei:publisher",
      "ex:corpus": "//tei:keywords[@n='corpus']/tei:term",
      "ex:authorGroup": "//tei:keywords[@n='authorGroup']/tei:term",
      "ex:workGroup": "//tei:keywords[@n='workGroup']/tei:term",
      "ex:work": ".//tei:titleStmt/tei:title[@type='main']"
    }
  },
  "overrides": {
    "W0001-lemma.xml": {
      "ex:workTitle": ".//tei:msDesc/tei:msContents/tei:title"
    },
    "^ISAAC_.*\\.xml$": {
      "dc:author": ".//tei:sourceDesc/tei:bibl/tei:author"
    }
  }
}
