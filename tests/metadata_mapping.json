{
  "default": {

    "namespaces": {
      "tei": "http://www.tei-c.org/ns/1.0",
      "dc": "http://purl.org/dc/terms/",
      "ex": "http://example.com/",
      "xml": "http://www.w3.org/XML/1998/namespace"
    },
    "properties": {
      "dc:title": ".//tei:titleStmt/tei:title[@type='main']",
      "dc:author": ".//tei:titleStmt/tei:author",
      "dc:description": ".//tei:sourceDesc//tei:title[@type='subtitle']",
      "dc:date": [
        ".//tei:sourceDesc//tei:date",
        ".//tei:editionStmt//tei:date"
      ],
      "dc:pubPlace": ".//tei:sourceDesc//tei:pubPlace",
      "dc:publisher": ".//tei:sourceDesc//tei:publisher"
    },
    "keywords_xpath": ".//tei:profileDesc/tei:textClass/tei:keywords",
    "multi_lang_extensions": {
      "ex:corpus": "$keywords_xpath[@xml:lang=$lang]/tei:term[@ana='corpus']",
      "ex:authorgroup": "$keywords_xpath[@xml:lang=$lang]/tei:term[@ana='authorgroup']",
      "ex:workgroup": "$keywords_xpath[@xml:lang=$lang]/tei:term[@ana='workgroup']",
      "ex:work": "$keywords_xpath[@xml:lang=$lang]/tei:term[@ana='work']"
    }
  }
}
